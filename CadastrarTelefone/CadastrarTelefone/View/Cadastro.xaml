        mc:Ignorable="d"
        Title="Cadastro" Height="211.5" Width="299">
    <Grid>
        <Button Content="Salvar" HorizontalAlignment="Left" Margin="117,112,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
